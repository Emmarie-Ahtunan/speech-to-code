{
	"name": "speech2code.editors.vscode",
	"displayName": "Spoken",
	"description": "Code With Voice - The default VSCode driver for Spoken",
	"publisher": "august",
	"version": "0.0.1",
	"icon": "icon-brain.png",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/pedrooaugusto/speech-to-code.git"
	},
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "spoken.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc --watch -p ./",
		"watch1": "tsc-watch  --onSuccess \"npm run copy-robot\" -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"copy-robot": "cp ./out/robot-vscode.js /c/Users/yuram/.vscode/extensions/august.spoken-0.0.1/out/robot-vscode.js",
		"vscode-install": "bash ./tools.sh"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/node-ipc": "^9.1.3",
		"@types/vscode": "^1.54.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"tsc-watch": "^4.2.9",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"node-ipc": "^9.1.4"
	}
}
