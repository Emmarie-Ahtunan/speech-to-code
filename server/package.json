{
  "name": "speech-to-code.server",
  "version": "0.0.1",
  "description": "Speech to Code NodeJS server",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "set GOOGLE_APPLICATION_CREDENTIALS=key.json&& node app.js",
    "build": "echo \"Nothing to build!\"",
    "prestart": "npm run build-spoken && sass public/assets/styles.scss:public/assets/styles.css --no-source-map",
    "watchscss": "sass --watch public/assets/styles.scss:public/assets/styles.css --no-source-map",
    "build-spoken": "cd ../spoken && npm run build"
  },
  "keywords": [
    "Speech-to-code"
  ],
  "dependencies": {
    "@google-cloud/speech": "^4.2.0",
    "spoken": "file:../spoken",
    "express": "^4.17.1",
    "socket.io": "^3.1.1"
  },
  "author": "Pedro Silva",
  "license": "GPL-3.0-or-later"
}
