{
  "name": "speech2code.client",
  "displayName": "Speech to Code",
  "productName": "speech2code",
  "version": "0.0.1",
  "description": "Speech to Code electron client implmentation",
  "author": "Pedro Silva",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "build": {
    "appId": "com.august.speech2code.client",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "icons",
      "output": "bin"
    },
    "publish": null
  },
  "scripts": {
    "test": "tsc && node dist/editors/vscode/index.test.js",
    "start": "electron .",
    "prestart": "bash ./tools.sh",
    "pack": "electron-builder -w zip"
  },
  "keywords": [
    "Speech-to-code"
  ],
  "dependencies": {
    "node-ipc": "9.1.4"
  },
  "devDependencies": {
    "electron": "11.2.3",
    "@types/node-ipc": "9.1.3",
    "electron-builder": "22.10.5"
  }
}
